- t_scope 'controllers.admin.prefabs.delete' do
  - title t(:title)
  - form_tag admin_prefab_path(@prefab), :method => :delete do |f|
    %p.center= t(:confirm_delete, :prefab => @prefab.name)
    %p.center
      = submit_tag t(:yes).capitalize
      &nbsp;
      = link_to t(:no).capitalize, admin_templates_path(:type => flash[:templates_type])